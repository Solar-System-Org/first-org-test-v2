pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo "Github"
            }
        }

        stage('Build') {
            steps {
                echo "Building the application..."
   
            }
        }

        stage('Test') {
            steps {
                echo "Running unit tests..."
          
            }
        }

        stage('Archive') {
            steps {
                echo "Archiving build artifacts..."
            }
        }
    }
}
